PanelWidgetClass UIActionText {
 size 1 30
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 scriptclass "UIActionText"
 {
  FrameWidgetClass action_wrapper {
   ignorepointer 1
   size 1 1
   position 0 0
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   {
    TextWidgetClass action_label {
     size 1 20
     valign center_ref
     text "{LABEL}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" 10 0
     "exact text" 1
     "exact text size" 16
     "bold text" 1
     vexactsize 1
    }
    TextWidgetClass action {
     size 1 20
     halign right_ref
     valign center_ref
     text "{TEXT}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" -10 0
     "exact text" 1
     "exact text size" 16
     "text halign" right
     vexactsize 1
    }
   }
  }
  FrameWidgetClass action_wrapper_disable {
   visible 0
   ignorepointer 1
   color 0.1569 0.1569 0.1569 0.902
   size 1 1
   position 0 0
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   style rover_sim_colorable
  }
 }
}