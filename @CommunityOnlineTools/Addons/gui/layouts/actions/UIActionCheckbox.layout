PanelWidgetClass action_checkbox_wrapper {
 size 1 30
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 valign center_ref
 scriptclass "UIActionCheckbox"
 {
  TextWidgetClass action_label {
   size 1 20
   halign left_ref
   valign center_ref
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 1
   text "{LABEL}"
   font "gui/fonts/sdf_MetronLight24"
   "text offset" 10 0
   "exact text" 1
   "exact text size" 16
   "bold text" 1
  }
  CheckBoxWidgetClass action_checkbox {
   visible 1
   ignorepointer 0
   keepsafezone 0
   color 1 1 1 1
   size 20 20
   halign right_ref
   valign center_ref
   hexactpos 0
   vexactpos 0
   hexactsize 1
   vexactsize 1
   scaled 0
   style Default
   checked 0
  }
 }
}