PanelWidgetClass UIActionSelectBox {
 size 1 30
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 scriptclass "UIActionSelectBox"
 {
  FrameWidgetClass action_wrapper {
   ignorepointer 1
   position 0 0
   size 1 1 
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   {
    TextWidgetClass action_label {
     size 1 20
     valign center_ref
     text "{LABEL}"
     font "gui/fonts/sdf_MetronLight24"
     "text offset" 10 0
     "exact text" 1
     "exact text size" 16
     "bold text" 1
     vexactsize 1
    }
    FrameWidgetClass action {
     position 0.3 0
     size 0.7 20 
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 1
    }
   }
  }
  FrameWidgetClass action_wrapper_disable {
   visible 0
   ignorepointer 1
   color 0.1569 0.1569 0.1569 0.902
   size 1 1
   position 0 0
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   style rover_sim_colorable
  }
 }
}